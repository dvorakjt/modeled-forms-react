    "lint": "node check-for-src.js && eslint --ext .ts .",
    "format": "node check-for-src.js && prettier --write \"**/*.+(js|ts|json)\"",
    "build-storybook": "node check-for-src.js && storybook build",
    "build": "node check-for-src.js && tsup"
  },
  "files": [
    "/dist",
    "check-for-src.js"
  ],
  "repository": {
    "type": "git",
