  "description": "Powerful MVC-inspired form management for React with declarative syntax and visually unopinionated, fully customizable components.",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "type": "module",
  "scripts": {
    "storybook": "node check-for-src.js && storybook dev -p 6006",
